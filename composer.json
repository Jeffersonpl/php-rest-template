{
  "name": "byjg/resttemplate",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "require": {
    "byjg/config": "2.0.*",
    "byjg/micro-orm": "2.0.*",
    "byjg/restserver": "2.1.*",
    "byjg/jwt-wrapper": "1.0.*"
  },
  "autoload": {
    "psr-4": {
      "RestTemplate\\": "src/project/",
      "Framework\\": "src/fw/"
    }
  },
  "scripts": {
    "migrate": "Framework\\Scripts::migrate",
    "build": "Framework\\Scripts::build",
    "restdocs": "Framework\\Scripts::genRestDocs",
    "post-create-project-cmd": "Framework\\PostCreateScript::run"
  },
  "require-dev": {
    "zircote/swagger-php": "2.0.*",
    "byjg/swagger-test": "1.0.*",
    "byjg/migration": "2.0.*"
  }
}
